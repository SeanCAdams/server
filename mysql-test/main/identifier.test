#
# Identifiers are accent sensitive and case insensitive,
# and there are usually only two variants of a letter (capital and small)
# having equal octet length in utf8.
#
# There are a few exceptions (coming from Unicode casefolding rules)
# changing octet length during casefolding:
#
# U+0130 LATIN CAPITAL LETTER I WITH DOT ABOVE (2 bytes in utf8) is equal to
# U+0049 LATIN CAPITAL LETTER I                (1 byte in utf8)
# U+0069 LATIN SMALL LETTER I                  (1 byte in utf8)
#
# U+212A KELVIN SIGN                           (3 bytes in utf8) is equal to
# U+004B LATIN CAPITAL LETTER  K               (1 byte in utf8)
# U+006B LATIN CAPITAL LETTER  K               (1 byte in utf8)
#
# U+212B ANGSTROM SIGN                         (3 bytes in utf8) is equal to
# U+00C5 LATIN CAPITAL LETTER WITH RING ABOVE  (2 bytes in utf8)
# U+00E5 LATIN SMALL LETTER WITH RING ABOVE    (2 bytes in utf8)
#
# U+2126 OHM SIGN                              (3 bytes in utf8) is eqaul to
# U+03A9 GREEK CAPITAL LETTER OMEGA            (2 bytes in utf8)
# U+03C9 GREEK SMALL LETTER OMEGA              (2 bytes in utf8)
#

#
# Testing "U+0130 CAPITAL LETTER I WITH DOT ABOVE" versus letters I and i.
#


# Column
SET NAMES utf8;
--error ER_DUP_FIELDNAME
CREATE TABLE t1 (I int, İ int);


# Index
--error ER_DUP_KEYNAME
CREATE TABLE t1 (a int, b int, KEY I(a), KEY İ(b));


